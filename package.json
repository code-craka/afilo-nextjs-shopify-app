{
  "name": "afilo-nextjs-app",
  "version": "2.2.0",
  "description": "A cutting-edge digital products platform specializing in software products - Built with Next.js 15, TypeScript, Stripe, and Neon PostgreSQL",
  "homepage": "https://app.afilo.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/code-craka/afilo-nextjs-shopify-app.git"
  },
  "bugs": {
    "url": "https://github.com/code-craka/afilo-nextjs-shopify-app/issues"
  },
  "author": {
    "name": "Rihan",
    "url": "https://github.com/code-craka"
  },
  "license": "MIT",
  "keywords": [
    "nextjs",
    "stripe",
    "digital-marketplace",
    "typescript",
    "tailwindcss",
    "neon-postgresql",
    "software-products",
    "digital-commerce",
    "license-management",
    "saas-platform"
  ],
  "engines": {
    "node": ">=18.17.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.6",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "analyze": "ANALYZE=true next build",
    "db:setup": "psql $DATABASE_URL -f scripts/setup-user-profiles.sql",
    "db:setup-neon": "psql $DATABASE_URL -f scripts/setup-user-profiles.sql",
    "auth:setup": "echo 'Please follow the setup guide in AUTHENTICATION_SETUP_GUIDE.md'"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.34.0",
    "@neondatabase/serverless": "^1.0.2",
    "@prisma/adapter-neon": "^6.18.0",
    "@prisma/client": "^6.18.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@react-email/components": "^0.5.7",
    "@react-email/render": "^1.4.0",
    "@react-pdf/renderer": "^4.3.1",
    "@stripe/react-stripe-js": "^5.2.0",
    "@stripe/stripe-js": "^8.1.0",
    "@supabase/postgrest-js": "^2.76.1",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.6",
    "bcrypt": "^6.0.0",
    "canvas-confetti": "^1.9.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.544.0",
    "next": "16.0.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.6.0",
    "recharts": "^3.3.0",
    "resend": "^6.2.2",
    "server-only": "^0.0.1",
    "stripe": "^19.1.0",
    "svix": "^1.80.0",
    "tailwind-merge": "^3.3.1",
    "ws": "^8.18.3",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/bcrypt": "^6.0.0",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^20.19.23",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "^4.0.3",
    "@vitest/ui": "^4.0.3",
    "eslint": "^9.38.0",
    "eslint-config-next": "15.5.4",
    "markdownlint": "^0.38.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prisma": "^6.18.0",
    "tailwindcss": "^4.1.16",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.3"
  }
}
